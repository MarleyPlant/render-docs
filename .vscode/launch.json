{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node_modules/moxygen/bin/moxygen.js",
            "args": ["--html-anchors", "--templates", "${workspaceFolder}/templates/cpp", "--output", "${workspaceFolder}/build/md/api.md", "${workspaceFolder}/build/xml/"]
            // "args": ["--html-anchors", "--classes" , "--templates", "${workspaceFolder}/templates/cpp", "--output", "${workspaceFolder}/build/md/api_%s.md", "${workspaceFolder}/build/xml/"]
        }
    ]
}